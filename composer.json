{
    "name": "lotgd/module-res-charstats",
    "description": "This module adds basic stats to the default character stats of lotgd/core",
    "type": "lotgd-module",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Res\\Charstats\\": "src/",
            "LotGD\\Module\\Res\\Charstats\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {"type": "path", "url": "../*"}
    ],
    "require": {
        "lotgd/core": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/yaml": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
