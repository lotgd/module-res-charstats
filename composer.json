{
    "name": "lotgd/module-res-charstats",
    "description": "This module adds basic stats to the default character stats of lotgd/core",
    "type": "lotgd-module",
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Res\\Charstats\\": "src/",
            "LotGD\\Module\\Res\\Charstats\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://raw.githubusercontent.com/lotgd/packages/master/build/packages.json"
        }
    ],
    "require": {
        "lotgd/core": ">=0.5.0-alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1|^5.1",
        "phpunit/dbunit": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
